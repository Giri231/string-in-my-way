int main()
{
    string s;
    getline(cin,s);
    int n;
    n=s.length();
    char temp;
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(s[i]>s[j])
            
            {
                temp=s[i];
                s[i]=s[j];
                s[j]=temp;
            }
        }
    }
    
   cout<<s;
    
}
