#include<iostream>

using namespace std;

int main()
{
    string s;
    getline(cin,s);
    int n;
    n=s.length();
    char temp;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n-1;j++)
        {
            if(s[j]>s[j+1])
            
            {
                temp=s[j];
                s[j]=s[j+1];
                s[j+1]=temp;
            }
        }
    
        
    }
   cout<<s;
    
}
